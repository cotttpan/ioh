{
  "name": "ioh",
  "description": "Typescript friendly Immutable Object Helper functions",
  "version": "0.0.6",
  "author": "cotto",
  "license": "MIT",
  "homepage": "https://github.com/cotttpan/ioh",
  "repository": {
    "type": "git",
    "url": "https://github.com/cotttpan/ioh"
  },
  "bugs": {
    "url": "https://github.com/cotttpan/ioh/issues"
  },
  "files": [
    "es",
    "dist",
    "README.md",
    "LICENCE"
  ],
  "jsnext:main": "es/index.js",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "immutable",
    "typescript"
  ],
  "dependencies": {
    "@cotto/utils.ts": "^0.0.3"
  },
  "devDependencies": {
    "@cotto/tslint-config": "^0.0.5",
    "@types/jest": "^20.0.7",
    "@types/node": "^8.0.24",
    "@types/sinon": "^2.3.3",
    "jest": "^20.0.4",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "ts-jest": "^20.0.10",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:dist": "tsc",
    "build:es": "tsc --module es2015 --outDir es",
    "clean": "rimraf ./dist ./es",
    "lint": "tslint src/**/*.ts",
    "myprepublish": "run-s clean build",
    "mypublish": "npm run myprepublish && npm publish",
    "test": "NODE_EVN=test jest --watch"
  }
}