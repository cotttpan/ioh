{
  "name": "ioh",
  "description": "Typescript friendly Immutable Object Helper functions",
  "version": "0.0.6",
  "author": "cotto",
  "license": "MIT",
  "homepage": "https://github.com/cotttpan/ioh",
  "repository": {
    "type": "git",
    "url": "https://github.com/cotttpan/ioh"
  },
  "bugs": {
    "url": "https://github.com/cotttpan/ioh/issues"
  },
  "files": [
    "es",
    "dist",
    "README.md",
    "LICENCE"
  ],
  "jsnext:main": "es/index.js",
  "main": "dist/index.js",
  "module": "es/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "immutable",
    "typescript"
  ],
  "dependencies": {
    "@cotto/utils.ts": "^0.2.11"
  },
  "devDependencies": {
    "@cotto/tslint-config": "^0.1.4",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.0",
    "@types/sinon": "^4.3.3",
    "jest": "^22.4.3",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "sinon": "^5.0.7",
    "ts-jest": "^22.4.6",
    "tsconfig-paths": "^3.3.2",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:dist": "tsc",
    "build:es": "tsc --module es2015 --outDir es",
    "clean": "rimraf ./dist ./es",
    "lint": "tslint src/**/*.ts",
    "myprepublish": "run-s clean build",
    "mypublish": "npm run myprepublish && npm publish",
    "test": "NODE_EVN=test jest --watch"
  }
}
